    <title>HOME</title>

    <link href="https://fonts.googleapis.com/css?family=Lato:400,700" rel="stylesheet">

    <link rel="stylesheet" href="css/bootstrap.css">

    <link rel="stylesheet" href="css/aos.css">

    <link rel="stylesheet" href="css/font-awesome.min.css">

    <link rel="stylesheet" href="css/style.css">
    <header class="header">
        <div class="navigation">
            <nav class="navbar navbar-expand-lg">
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item active">
                            <a class="nav-link" href="http://localhost:3000">Home</a>
                        </li>
                    </ul>
                </div>
            </nav>
        </div>
    </header>
    <section class="main">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-8 col-sm-10 mx-auto">
                    <div class="theiaStickySidebar">
                        <div class="main__content">
                            <img src="" alt="header main image one" class="main__image">
                            <div class="main__text">
                                <h3 class="heading-tertiary mb-4"></h3>
                                <div class="main__link">
                                    <h5>May, 15 2019</h5>
                                    <div class="main__icon main__refresh">
                                        Refresh
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="main__subcard">
                            <div class="row">
                                <div class="col-md-6 col-sm-12 mx-auto">
                                    <img src="" alt="article card image" class="main__subcard--image">
                                </div>
                                <div class="col-md-6 col-sm-12 mx-auto align-self-center">
                                    <div class="main__subcard--text main__center__content">
                                        <h3 class="heading-quaternary weight"  style="cursor: pointer;"></h3>
                                        <p class="paragraph"></p>
                                        <div class="main__link">
                                            <p class="heading-tag"><a href="javaScript:void(0);">Refresh Other</a></p>
                                            <p class="heading-tag heading-detail"><a href="javaScript:void(0);">Go Detail</a></p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <section class="main-banner">
